#import <UIKit/UIKit.h>
#define kLeftRightSpace 30 //左右空隙
#define kUpDownSpace 0    //上下空隙

#define kxLabelWidth 25 //x标注宽度
#define kxLabelHeight 12 //x标注高度

#define PI 3.1415926
#import "PortfolioStrategyHelper.h"
@interface CoordinateGraphs : UIView
/*
 方法描述:
 初始化方法
 
 参数说明:
 frame:页面大小
 p_dataArr：期权数据
 p_fLagModel：第一条腿数据
 p_sLagModel：第二条腿数据
 p_type：组合策略
 p_compoundModelType：组合方式
 返回结果:
 
 
 */
- (id)initWithFrame:(CGRect)frame
         andDataArr:(NSArray *)p_dataArr
       andFLagModel:(OptionsInforModel *)p_fLagModel
       andSLagModel:(OptionsInforModel *)p_sLagModel
andStrategySelectionType:(Enum_StrategySelectionType)p_type
 andCompundModeType:(Enum_CompoundMode)p_compoundModelType;
@end
